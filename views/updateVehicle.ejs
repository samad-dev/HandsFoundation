                    var d3_rs = "<%= data.total %>";
                    var t3_rs = parseInt(d3_rs);
                    d3.push(t3_rs);
                    var d3_cng = "<%= data.cng %>";
                    var t3_cng = parseInt(d3_cng);
                    d3_2.push(t3_cng);
                    var d3_date = "<%= data.as_on %>";
                    d3_label.push(d3_date.split(' ')[1]+" "+ d3_date.split(' ')[3]);
                </script>
                <% }) %>
            <% } %>   
            <% if(data4.length!=0){ var
                i=1; var j=1;
                data4.forEach(function(data)
