                        d4_odo = 0;
                    }
                    var t4_odo = parseInt(d4_odo);
                    d4_2.push(t4_odo);
                    var d4_date = "<%= data.as_on %>";
                    d4_label.push(d4_date.split(' ')[1]+" "+ d4_date.split(' ')[3]);
                </script>
                <% }) %>
            <% } %>   
                    
