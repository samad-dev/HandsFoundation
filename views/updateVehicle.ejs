                        d4_rs = 0;
                    }
                    var t4_rs = parseInt(d4_rs);
                    d4.push(t4_rs);
                    var d4_odo = "<%= data.ReportedDistance %>";
                    console.log(d4_odo);
                    if(d4_odo=="")
