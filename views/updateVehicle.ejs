                    rs_gr.push(t_rs);
                    var rs_date = "<%= data.as_on %>";
                    rs_gr_label.push(rs_date.split(' ')[1]+" "+ rs_date.split(' ')[3]);
                </script>
                <% }) %>
            <% } %> 
            <% if(data3.length!=0){ var
                i=1; var j=1;
                data3.forEach(function(data)
